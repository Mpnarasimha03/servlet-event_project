<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Creating</title>
</head>
<body>

	<h1>Create the Event</h1>
	<form action="create">
		<input type="text" name="id" placeholder="Enter the event ID">
		<input type="text" name="title" placeholder="Enter the event Title">
		<input type="text" name="loc" placeholder="Enter the event Location">
		<input type="text" name="date" placeholder="Enter the event Date">
		<input type="text" name="guest" placeholder="Enter the event Chief Guest ">
		<button type="submit">SUBMIT</button>
	</form>


</body>
</html>